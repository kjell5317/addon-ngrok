{
  "name": "Ngrok",
  "version": "0.1.5",
  "slug": "ngrok",
  "url": "https://github.com/kjell5317/addon-ngrok",
  "description": "This addon creates Ngrok tunnels to your Home Assistant instance",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "4040/tcp": 4040
  },
  "options": {
    "ng_token": null,
    "fn_user": null,
    "fn_pwd": null,
    "fn_domain": null,
    "port": 8123
  },
  "schema": {
    "ng_token": "password",
    "fn_user": "email",
    "fn_pwd": "password",
    "fn_domain": "url",
    "port": "port"
  },
  "map": ["config"]
}
