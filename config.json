{
  "name": "Ngrok",
  "version": "1.0.1",
  "slug": "ngrok",
  "url": "https://github.com/kjell5317/addon-ngrok",
  "description": "This addon creates Ngrok tunnels to your Home Assistant instance",
  "arch": ["armhf", "armv7"],
  "startup": "services",
  "boot": "auto",
  "options": {
    "ng_token": null,
    "fn_user": null,
    "fn_pwd": null,
    "fn_domain": null,
    "address": "http://localhost:8123"
  },
  "schema": {
    "ng_token": "password",
    "fn_user": "email",
    "fn_pwd": "password",
    "fn_domain": "str",
    "address": "url"
  },
  "map": ["config"],
  "init": false
}
